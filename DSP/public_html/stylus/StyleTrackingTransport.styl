.battery_none,
.battery_0,
.battery_1,
.battery_2,
.battery_3,
.battery_4,
.battery_5,
.battery_6,
.battery_7,
.battery_8
	height 3.8em
	width 2.2em
	background-color transparent
	box-sizing border-box
	border .3em #000 solid
	float left
	display block
