  	height .6em
  	margin-top 2.4em

.battery_3::after
  	height 1em
  	margin-top 2em

.battery_4::after
  	height 1.6em
  	margin-top 1.4em

.battery_5::after
  	height 1.9em
  	margin-top 1.1em

.battery_6::after
  	height 2.5em
  	margin-top 0.5em

.battery_7::after
  	height 2.8em
  	margin-top 0.2em
  	
.battery_8::after 
    height 2.8em
    margin-top .2em
