
@import 'nib'

gris_claro = #818181
gris_fondo = #3d4044
gris_oscuro = #333333
green_status = #16a765
blue_status = #12aadf
red_fondo = #E43131

.databox_principal
    background #fff
    font-size 13px
    color #333
    border 1px solid #e3e3e3
    padding 8px 15px 15px 15px
    margin 8px 10px 8px 10px
    position relative
    border-radius 0.25em
    box-shadow 1px 2px 2px 0px rgba(0,0,0,0.25)

.databox_interno_color,
.databox_interno_color_green,
.databox_interno_color_red,
.databox_interno_color_blue,
.databox_interno
    display inline-block
    width:15%;
    border 1px solid #e3e3e3
    padding 8px 15px 15px 15px
    margin 8px 5px 8px 5px;
    border-radius 0.25em
    box-shadow 1px 2px 2px 0px rgba(0,0,0,0.25)
    
    
    font-family 'Varela Round', sans-serif
    font-size 9px
    p
        color #59789C
        margin 0px
        text-align center
    hr
        border 0px
        border-bottom 1px solid #D3D3D3;
    span
        display inline-block
        padding-left 5px
        text-align center
        p
            display inline-block
            color #59789C
            margin 0px
            padding-right 2px
            text-align center

.databox_interno_color
    border-left 5px solid #e3e3e3
    
.databox_interno_color_green
    border-left 5px solid #16a765
    
.databox_interno_color_red
    border-left 5px solid #E43131

.databox_interno_color_blue
    border-left 5px solid #12aadf


.databox_list 
    background #f9f9f9
    font-size 13px
    color #333
    border-bottom 1px solid #e3e3e3
    padding 5px

.databox_list_pointer 
    background #fff
    font-size 13px
    color #333
    border 1px solid #e3e3e3
    padding 8px 15px 15px 15px
    margin 8px 10px 8px 10px
    position relative
    border-radius 0.25em
    box-shadow 1px 2px 2px 0px rgba(0,0,0,0.25)
    border-left 5px solid #e3e3e3

.databox_list_pointer.x-item-selected
    //background  #E8E8E8
    border-bottom 2px solid red_fondo
    border-left 5px solid red_fondo

.x-view-item-focused
    outline: 1px dashed #ccc !important
    outline-offset -1px
    border-bottom 2px solid red_fondo
    border-left 5px solid red_fondo

.databox_list_pointer:hover 
    background none repeat scroll 0 0 #eceff5
    cursor pointer  

.databox_list:hover 
    background none repeat scroll 0 0 #eceff5

.databox_bar
    padding-bottom 4px

.databox_title,
.databox_title_reg
    font-family Arial, Helvetica, sans-serif
    font-weight bold
    margin-left 25px
