.databox_nodedad_titulo_hst,
.databox_menu_novedad_titulo
    background  #708092
    color #fff
    font-family 'Varela Round', sans-serif
    width 100%
    font-size 13px
    font-weight bold
    padding 8px 15px 15px 15px
    margin 8px 10px 8px 10px

.databox_menu_novedad_titulo
    height 34px
    clear bold
    display inline-block
    .databox_ttl
        display inline-block
    .databox_menu_novedad,
    .databox_add_novedad
        display inline-block
        a:hover
            display inline-block
            //background-color #f0f0f0
            background linear-gradient(#f0f0f0, #fff) repeat scroll 0 0 rgba(0, 0, 0, 0)
            /*background-image linear-gradient(#fff, #f0f0f0)
            background-image linear-gradient(#fff, #f0f0f0)*/
            box-shadow 0 1px 2px 0 rgba(0, 0, 0, 0.22) inset
            //border-color #A9A9A9
            box-shadow 0 1px 7px 0 rgba(0, 0, 0, 0.5) inset
            text-shadow 0 1px rgba(0, 0, 0, 0.6)
        a:active
            display inline-block
            background-image linear-gradient(#bb1618, #f0f0f0)
            box-shadow 0 1px 2px 0 rgba(0, 0, 0, 0.22) inset
            border 1px solid #f0f0f0
            background linear-gradient(#fff, #f0f0f0) repeat scroll 0 0 rgba(0, 0, 0, 0)
        a
            border-radius 3px
            background linear-gradient(#fff, #f0f0f0) repeat scroll 0 0 rgba(0, 0, 0, 0)
            border 1px solid #ccc
            color #5f5f5f
            cursor pointer
            font-weight bold
            padding 8px 14px 7px
            text-shadow 0 1px #fff
            white-space nowrap
            width 35px
            height 30px
            margin-top 0px        
            display inline-block
            position absolute
            top 2px
            margin-right 3px
            em
                height 16px
                width 16px
                display inline-block
                position relative
                vertical-align middle
                left -5px
                top 0px
.databox_menu_novedad
    a
        right 0px
        em
            background url("/images/icon/webapp-desktop.png") no-repeat scroll -366px -550px rgba(0, 0, 0, 0)
    
.databox_add_novedad
    a
        right 37px
        em
            background url("/images/icon/webapp-desktop.png") no-repeat scroll -160px -570px rgba(0, 0, 0, 0)


.databox_status_on,
.databox_status_off,
.databox_cerrado
    width 43px
    height 15px
    display inline-block
    position absolute
    top 0px
    right 5px
    text-align center
    //border-radius 0.25em
    //border-bottom 1px solid #e3e3e3
    font-size 9px
    color #fff
    padding-top 2px

.databox_status_on
    background green_status

.databox_status_off
    background-color rgba(0, 0, 0, 0)
    -webkit-transition background-color 2s
    -moz-transition background-color 2s
    transition background-color 2s

.databox_cerrado
    background #F08080

.databox_status_msn_on,
.databox_status_msn_off
    width 40px
    height 15px
    display inline-block
    position absolute
    top 5px
    right 38px
    border-radius 0.25em
    //border-bottom 1px solid #e3e3e3
    font-size 10px
    color #fff
    padding 2px 0px 1px 5px 

.databox_status_msn_on
    background #B0C4DE

.databox_status_msn_off
    background-color rgba(0, 0, 0, 0)
    -webkit-transition background-color 4s
    -moz-transition background-color 4s
    transition background-color 4s

.box-display-reg-on
    display block

.box-display-reg-off
    display none

