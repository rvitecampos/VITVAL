        display inline-block
        padding 2px

.databox_title
    font-size 11px 

.databox_title_reg
    font-size 15px 

.databox_date
    font-family Arial, Helvetica, sans-serif
    font-size 10px 
    color #708090
    font-weight bold
    margin-left 25px
    span
        display inline-block
        padding-left 18px
        padding-bottom 4px
    .dbx_user
        color #708092
        padding-left 2px
    .dbx_fecha
        background-image url('/images/icon/cal-icon.png')
        background-size 13px 13px
        background-repeat no-repeat
        margin-left 6px
    .dbx_hora
        background-image url('/images/icon/clock-icon.png')
        background-size 13px 13px
        background-repeat no-repeat
